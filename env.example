# ============================================================================
# Environment Variables for Qwen3VL RAG System
# ============================================================================
# Copy this file to .env and adjust values as needed
# Environment variables override config.yaml settings
# ============================================================================

# ============================================================================
# EMBEDDING API
# ============================================================================
EMBEDDING_API_URL=http://100.126.235.19:8888/v1/embeddings

# ============================================================================
# RERANKER API (Optional - for two-stage retrieval)
# ============================================================================
# Enable when you have Qwen3-VL-Reranker endpoint available
RERANKER_ENABLED=false
RERANKER_API_URL=http://100.126.235.19:8888/v1/rerank

# ============================================================================
# VECTOR STORE
# ============================================================================
# Type: faiss or milvus
VECTOR_STORE_TYPE=faiss

# ============================================================================
# MILVUS CONFIGURATION (Only needed if VECTOR_STORE_TYPE=milvus)
# ============================================================================
# Connection
MILVUS_HOST=localhost
MILVUS_PORT=19530

# Authentication (optional)
MILVUS_USER=
MILVUS_PASSWORD=

# Database
MILVUS_DB_NAME=default
MILVUS_COLLECTION_NAME=rag_documents

# Index Configuration
MILVUS_INDEX_TYPE=IVF_FLAT
MILVUS_METRIC_TYPE=L2

# ============================================================================
# PERFORMANCE TUNING (Optional - uses config.yaml defaults if not set)
# ============================================================================
# RAG_CHUNK_SIZE=500
# RAG_CHUNK_OVERLAP=50
# RAG_TOP_K=5
# RAG_PDF_DPI=150
# RAG_BATCH_SIZE=32

# ============================================================================
# LOGGING (Optional)
# ============================================================================
# LOG_LEVEL=INFO
# LOG_FILE=logs/rag_system.log
